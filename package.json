{
  "name": "macrolog-webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "start": "node server.js",
    "build": "ng build",
    "heroku-postbuild": "./checkPostBuild.sh",
    "test": "ng test --code-coverage --watch=false",
    "test:local": "ng test --code-coverage --browsers=chrome",
    "lint": "ng lint",
    "cypress:open": "cypress open --config-file ./cypress.local.json",
    "cypress:local": "cypress run --config-file ./cypress.local.json",
    "cypress:integration": "cypress run --config-file ./cypress.integration.json",
    "start-server": "angular-http-server --path ./dist -p 4200",
    "integration": "start-server-and-test start-server http://localhost:4200 cypress:integration",
    "mock-server": "node mock/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@fortawesome/angular-fontawesome": "^0.10.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/operators": "^18.1.1",
    "@ngrx/store": "^20.1.0",
    "date-fns": "^2.25.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.15.1"
